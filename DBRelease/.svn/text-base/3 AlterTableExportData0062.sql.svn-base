		FROM INFORMATION_SCHEMA.COLUMNS
		where TABLE_NAME = 'AG_ExportData0062' 
		AND COLUMN_NAME = 'FechaOpenFilePdf')
BEGIN

		ALTER TABLE dbo.AG_ExportData0062 ADD FechaOpenFilePdf DATETIME 

END 
GO		


