
IF EXISTS(
SELECT TABLE_NAME, COLUMN_NAME 
		FROM INFORMATION_SCHEMA.COLUMNS
		where TABLE_NAME = 'AG_ExportData0062' 
		AND COLUMN_NAME = 'firmado')
BEGIN 
		ALTER TABLE dbo.AG_ExportData0062 DROP COLUMN firmado
END		 
GO	


